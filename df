[32m+[m[32m# Define here the models for your spider middleware[m
[32m+[m[32m#[m
[32m+[m[32m# See documentation in:[m
[32m+[m[32m# https://docs.scrapy.org/en/latest/topics/spider-middleware.html[m
[32m+[m
[32m+[m[32mfrom scrapy import signals[m
[32m+[m
[32m+[m[32m# useful for handling different item types with a single interface[m
[32m+[m[32mfrom itemadapter import is_item, ItemAdapter[m
[32m+[m
[32m+[m
[32m+[m[32mclass DatpiffSpiderMiddleware:[m
[32m+[m[32m    # Not all methods need to be defined. If a method is not defined,[m
[32m+[m[32m    # scrapy acts as if the spider middleware does not modify the[m
[32m+[m[32m    # passed objects.[m
[32m+[m
[32m+[m[32m    @classmethod[m
[32m+[m[32m    def from_crawler(cls, crawler):[m
[32m+[m[32m        # This method is used by Scrapy to create your spiders.[m
[32m+[m[32m        s = cls()[m
[32m+[m[32m        crawler.signals.connect(s.spider_opened, signal=signals.spider_opened)[m
[32m+[m[32m        return s[m
[32m+[m
[32m+[m[32m    def process_spider_input(self, response, spider):[m
[32m+[m[32m        # Called for each response that goes through the spider[m
[32m+[m[32m        # middleware and into the spider.[m
[32m+[m
[32m+[m[32m        # Should return None or raise an exception.[m
[32m+[m[32m        return None[m
[32m+[m
[32m+[m[32m    def process_spider_output(self, response, result, spider):[m
[32m+[m[32m        # Called with the results returned from the Spider, after[m
[32m+[m[32m        # it has processed the response.[m
[32m+[m
[32m+[m[32m        # Must return an iterable of Request, or item objects.[m
[32m+[m[32m        for i in result:[m
[32m+[m[32m            yield i[m
[32m+[m
[32m+[m[32m    def process_spider_exception(self, response, exception, spider):[m
[32m+[m[32m        # Called when a spider or process_spider_input() method[m
[32m+[m[32m        # (from other spider middleware) raises an exception.[m
[32m+[m
[32m+[m[32m        # Should return either None or an iterable of Request or item objects.[m
[32m+[m[32m        pass[m
[32m+[m
[32m+[m[32m    def process_start_requests(self, start_requests, spider):[m
[32m+[m[32m        # Called with the start requests of the spider, and works[m
[32m+[m[32m        # similarly to the process_spider_output() method, except[m
[32m+[m[32m        # that it doesnâ€™t have a response associated.[m
[32m+[m
[32m+[m[32m        # Must return only requests (not items).[m
[32m+[m[32m        for r in start_requests:[m
[32m+[m[32m            yield r[m
[32m+[m
[32m+[m[32m    def spider_opened(self, spider):[m
[32m+[m[32m        spider.logger.info('Spider opened: %s' % spider.name)[m
[32m+[m
[32m+[m
[32m+[m[32mclass DatpiffDownloaderMiddleware:[m
[32m+[m[32m    # Not all methods need to be defined. If a method is not defined,[m
[32m+[m[32m    # scrapy acts as if the downloader middleware does not modify the[m
[32m+[m[32m    # passed objects.[m
[32m+[m
[32m+[m[32m    @classmethod[m
[32m+[m[32m    def from_crawler(cls, crawler):[m
[32m+[m[32m        # This method is used by Scrapy to create your spiders.[m
[32m+[m[32m        s = cls()[m
[32m+[m[32m        crawler.signals.connect(s.spider_opened, signal=signals.spider_opened)[m
[32m+[m[32m        return s[m
[32m+[m
[32m+[m[32m    def process_request(self, request, spider):[m
[32m+[m[32m        # Called for each request that goes through the downloader[m
[32m+[m[32m        # middleware.[m
[32m+[m
[32m+[m[32m        # Must either:[m
[32m+[m[32m        # - return None: continue processing this request[m
[32m+[m[32m        # - or return a Response object[m
[32m+[m[32m        # - or return a Request object[m
[32m+[m[32m        # - or raise IgnoreRequest: process_exception() methods of[m
[32m+[m[32m        #   installed downloader middleware will be called[m
[32m+[m[32m        return None[m
[32m+[m
[32m+[m[32m    def process_response(self, request, response, spider):[m
[32m+[m[32m        # Called with the response returned from the downloader.[m
[32m+[m
[32m+[m[32m        # Must either;[m
[32m+[m[32m        # - return a Response object[m
[32m+[m[32m        # - return a Request object[m
[32m+[m[32m        # - or raise IgnoreRequest[m
[32m+[m[32m        return response[m
[32m+[m
[32m+[m[32m    def process_exception(self, request, exception, spider):[m
[32m+[m[32m        # Called when a download handler or a process_request()[m
[32m+[m[32m        # (from other downloader middleware) raises an exception.[m
[32m+[m
[32m+[m[32m        # Must either:[m
[32m+[m[32m        # - return None: continue processing this exception[m
[32m+[m[32m        # - return a Response object: stops process_exception() chain[m
[32m+[m[32m        # - return a Request object: stops process_exception() chain[m
[32m+[m[32m        pass[m
[32m+[m
[32m+[m[32m    def spider_opened(self, spider):[m
[32m+[m[32m        spider.logger.info('Spider opened: %s' % spider.name)[m
[1mdiff --git a/datpiff/pipelines.py b/datpiff/pipelines.py[m
[1mnew file mode 100644[m
[1mindex 0000000..ba3e43e[m
[1m--- /dev/null[m
[1m+++ b/datpiff/pipelines.py[m
[36m@@ -0,0 +1,26 @@[m
[32m+[m[32m# Define your item pipelines here[m
[32m+[m[32m#[m
[32m+[m[32m# Don't forget to add your pipeline to the ITEM_PIPELINES setting[m
[32m+[m[32m# See: https://docs.scrapy.org/en/latest/topics/item-pipeline.html[m
[32m+[m
[32m+[m
[32m+[m[32m# useful for handling different item types with a single interface[m
[32m+[m[32mfrom scrapy.exporters import CsvItemExporter[m
[32m+[m
[32m+[m[32mclass WriteItemPipeline(object):[m
[32m+[m
[32m+[m[32m    def __init__(self):[m
[32m+[m[32m        self.filename = 'datpiff.csv'[m
[32m+[m
[32m+[m[32m    def open_spider(self, spider):[m
[32m+[m[32m        self.csvfile = open(self.filename, 'wb')[m
[32m+[m[32m        self.exporter = CsvItemExporter(self.csvfile)[m
[32m+[m[32m        self.exporter.start_exporting()[m
[32m+[m
[32m+[m[32m    def close_spider(self, spider):[m
[32m+[m[32m        self.exporter.finish_exporting()[m
[32m+[m[32m        self.csvfile.close()[m
[32m+[m
[32m+[m[32m    def process_item(self, item, spider):[m
[32m+[m[32m        self.exporter.export_item(item)[m
[32m+[m[32m        return item[m
\ No newline at end of file[m
