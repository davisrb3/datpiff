[32m+[m[32m  scale_y_continuous(limits = c(0, 0.55), breaks = seq(0, 0.50, 0.1)) +[m[41m [m
[32m+[m[32m  scale_color_discrete(name = NULL, labels = c("Listens per View", "Downloads per View"))[m
