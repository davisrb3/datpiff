[32m+[m
[32m+[m[32m## Median Listens by Download Enabled status[m
[32m+[m[32m```{r}[m
[32m+[m[32mdata %>%[m
[32m+[m[32m  group_by(., release_month, download_enabled) %>%[m[41m [m
[32m+[m[32m  summarise(med_listens = median(listens)) %>%[m[41m [m
[32m+[m[32m  ggplot(., aes(x = release_month, y = med_listens, color = as.factor(download_enabled))) +[m[41m [m
[32m+[m[32m  geom_line() +[m[41m [m
[32m+[m[32m  ggtitle("Median Monthly Listens by Download Enabled Status") +[m
[32m+[m[32m  xlim(ymd("2017-01-01",NA)) + ylim(0,180000) +[m[41m [m
[32m+[m[32m  ylab("Median Monthly Listens") +[m[41m [m
[32m+[m[32m  xlab("Release Month") +[m
[32m+[m[32m  scale_color_discrete(name = NULL, labels = c("Downloads Disabled", "Downloads Enabled"))[m
[32m+[m[32m```[m
[32m+[m
[32m+[m[32m### Summarising Data by Artist percentile[m
[32m+[m[32m```{r}[m
[32m+[m[32mby_artist <- data %>%[m[41m [m
[32m+[m[32m  group_by(., release_year, artist) %>%[m[41m [m
[32m+[m[32m  summarise(., mixtapes = n(), download_enabled = sum(download_enabled), buy_enabled = sum(buy_enabled),[m[41m [m
[32m+[m[32m            total_views = sum(views), avg_views = mean(views),[m[41m [m
[32m+[m[32m            total_listens = sum(listens), avg_listens = mean(listens),[m
[32m+[m[32m            total_downloads = sum(downloads, na.rm = T), avg_downloads = mean(downloads)) %>%[m[41m [m
[32m+[m[32m  group_by(., release_year) %>%[m[41m [m
[32m+[m[32m  mutate(., ptile = ntile(desc(avg_views), 20))[m
[32m+[m
[32m+[m[32mannl_artist_ptile <- by_artist %>%[m[41m [m
[32m+[m[32m  select(., release_year, artist, ptile)[m
[32m+[m[41m  [m
[32m+[m[32mby_artist[m
[32m+[m
[32m+[m[32mdata_ptile <- inner_join(data, annl_artist_ptile)[m
[32m+[m
[32m+[m[32mdata_ptile[m
[32m+[m[32m```[m
[32m+[m
[32m+[m[32m### Download / Buy Rates by ptile[m
[32m+[m[32m```{r}[m
[32m+[m[32mdata_ptile %>%[m[41m [m
[32m+[m[32m  group_by(., release_month, ptile, download_enabled)[m
[32m+[m[32m```[m
[32m+[m
[32m+[m[32m### Total listens by annual artist percentiles (in 5%s) for the past 5 years (2015-2020)[m
[32m+[m[32m```{r}[m
[32m+[m[32mby_artist %>%[m[41m [m
[32m+[m[32m  filter(., between(release_year, 2015, 2020)) %>%[m[41m [m
[32m+[m[32m  ggplot(., aes(x = ptile, y = avg_listens)) +[m[41m [m
[32m+[m[32m  geom_histogram(stat = "identity")[m
[32m+[m[32m```[m
[32m+[m
[32m+[m[32m### Histogram[m
[32m+[m[32m```{r}[m
[32m+[m[32mhist_data <- data %>%[m[41m [m
[32m+[m[32m  filter(., between(release_year, 2015, 2020)) %>%[m[41m [m
[32m+[m[32m  mutate(., views_bucket = ifelse(views > 1E6, 1E6+1, views))[m
[32m+[m
[32m+[m[32mhist_avg <- mean(hist_data$views)[m
[32m+[m
[32m+[m[32mhist_breaks = seq(0, 10E5, 1E5)[m
[32m+[m[32mhist_labels = append(seq(0, 900, 100), "1000\nand Greater")[m
[32m+[m
[32m+[m[32mhistogram <- hist_data %>%[m[41m [m
[32m+[m[32m  ggplot(., aes(x = views_bucket)) +[m[41m [m
[32m+[m[32m  geom_histogram(breaks = seq(0, 1.05E6, 5E4)) +[m[41m [m
[32m+[m[32m  ggtitle("Frequency of View Counts Among Mixtapes (2015-2020)") +[m[41m [m
[32m+[m[32m  theme_minimal() +[m[41m [m
[32m+[m[32m  ylab("Count") +[m[41m [m
[32m+[m[32m  xlab("Number of Views (Thousands)") +[m[41m [m
[32m+[m[32m  scale_x_continuous(breaks = hist_breaks, labels = hist_labels) +[m[41m [m
[32m+[m[32m  stat_bin(breaks = seq(0, 1.05E6, 5E4), geom="text", aes(label=..count..), vjust=-0.5)[m[41m [m
[32m+[m
[32m+[m[32mhistogram[m
[32m+[m[32m```[m
[32m+[m
[32m+[m[32m### Same but log[m
[32m+[m[32m```{r}[m
[32m+[m[32mdata %>%[m[41m [m
[32m+[m[32m  filter(., between(release_year, 2015, 2020)) %>%[m[41m [m
[32m+[m[32m  ggplot(., aes(x = log(views))) +[m[41m [m
[32m+[m[32m  geom_density() +[m[41m [m
[32m+[m[32m  ggtitle("Density of Views Across Mixtapes (2015-2020)") +[m[41m [m
[32m+[m[32m  theme_minimal() +[m[41m [m
[32m+[m[32m  ylab("Density") +[m[41m [m
[32m+[m[32m  xlab("Number of Views") +[m[41m [m
[32m+[m[32m  scale_x_continuous(label = comma)[m
[32m+[m
[32m+[m[32m```[m
[32m+[m
[32m+[m[32m### Rate of 'Download Enabled' and 'Buy Enabled'[m
[32m+[m[32m```{r}[m
[32m+[m[32moptions_enabled <- data %>%[m[41m [m
[32m+[m[32m  group_by(., release_month) %>%[m[41m [m
[32m+[m[32m  summarise(., count = n(), download_enabled = sum(download_enabled), buy_enabled = sum(buy_enabled)) %>%[m[41m [m
[32m+[m[32m  mutate(., download_enabled_rate = download_enabled / count, buy_enabled_rate = buy_enabled / count) %>%[m
[32m+[m[32m  pivot_longer(., count:buy_enabled_rate)[m
[32m+[m
[32m+[m[32moptions_enabled_ptile <- data_ptile %>%[m[41m [m
[32m+[m[32m  group_by(., release_year, ptile) %>%[m[41m [m
[32m+[m[32m  summarise(., count = n(), download_enabled = sum(download_enabled), buy_enabled = sum(buy_enabled)) %>%[m[41m [m
[32m+[m[32m  mutate(., download_enabled_rate = download_enabled / count, buy_enabled_rate = buy_enabled / count) %>%[m
[32m+[m[32m  pivot_longer(., count:buy_enabled_rate)[m
[32m+[m
[32m+[m[32moptions_enabled_ptile[m
[32m+[m[32m```[m
[32m+[m
[32m+[m[32m```{r}[m
[32m+[m[32moptions_enabled_ptile%>%[m[41m [m
[32m+[m[32m  filter(name == "download_enabled_rate", ptile %in% c(1,5,10,15,20)) %>%[m[41m [m
[32m+[m[32m  ggplot(., aes(x = release_year, y = value)) +[m
[32m+[m[32m  geom_line(aes(color = as.factor(ptile))) +[m[41m [m
[32m+[m[32m  ggtitle("Mixtapes with Purchase and Download Options (2015 - 2020)") +[m
[32m+[m[32m  theme_minimal() +[m[41m [m
[32m+[m[32m  xlim(2015,2020) +[m
[32m+[m[32m  ylab("Mixtapes")[m
[32m+[m
[32m+[m[32m```[m
[32m+[m
