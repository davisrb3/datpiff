[36m@@ -0,0 +1,442 @@[m
[32m+[m[32m---[m
[32m+[m[32mtitle: "Datpiff"[m
[32m+[m[32mauthor: "Rob Davis"[m
[32m+[m[32mdate: "11/2/2020"[m
[32m+[m[32moutput:[m[41m [m
[32m+[m[32m    html_notebook: default[m
[32m+[m[32m    html_document: default[m
[32m+[m[32m---[m
[32m+[m
[32m+[m[32m```{r setup, include=FALSE}[m
[32m+[m[32mknitr::opts_chunk$set(echo = TRUE)[m
[32m+[m[32m```[m
[32m+[m
[32m+[m[32m## Load Libraries[m
[32m+[m[32m```{r}[m
[32m+[m[32mlibrary(dplyr)[m
[32m+[m[32mlibrary(ggplot2)[m
[32m+[m[32mlibrary(tidyr)[m
[32m+[m[32mlibrary(lubridate)[m
[32m+[m[32mlibrary(VIM)[m
[32m+[m[32mlibrary(reshape2)[m
[32m+[m[32mlibrary(scales)[m
[32m+[m
[32m+[m[32moptions(dplyr.summarise.inform = FALSE)[m
[32m+[m[32m```[m
[32m+[m
[32m+[m[32m## Import Data[m
[32m+[m[32m```{r}[m
[32m+[m[32mraw.data_0 <- read.csv('../datpiff_2020-11-03_imputed.csv')[m
[32m+[m[32m```[m
[32m+[m
[32m+[m[32m#### Let's take a quick look at the data[m
[32m+[m[32m```{r}[m
[32m+[m[32msummary(raw.data_0)[m
[32m+[m[32m```[m
[32m+[m[32m#### Rename the first column which is screwed up for some reason[m
[32m+[m[32m#### Drop the description field for now, which doesn't have much significance or usability[m
[32m+[m[32m#### convert release_date to a date[m
[32m+[m[32m#### Change these (no/yes) fields to (0/1) (buy/download/streaming)_enabled[m
[32m+[m[32m#### Drop mixtapes with 0 tracks[m
[32m+[m[32m#### Drop mixtapes where listens > views or downloads > views (these are few and likely erroneous or fraudulent)[m
[32m+[m[32m#### All mixtapes have more than 0 listens, therefore 0 views is an error and should be imputed (set as NA for now)[m
[32m+[m[32m```{r}[m
[32m+[m[32mraw.data_1 <- raw.data_0 %>%[m[41m [m
[32m+[m[32m  rename(., added_by = 'Ã¯..added_by') %>%[m[41m [m
[32m+[m[32m  select(., !c(description)) %>%[m[41m [m
[32m+[m[32m  mutate(., release_date = mdy(release_date),[m
[32m+[m[32m         buy_enabled = ifelse(buy_enabled=='yes',1,0),[m
[32m+[m[32m         download_enabled = ifelse(download_enabled=='yes',1,0),[m
[32m+[m[32m         streaming_enabled = ifelse(streaming_enabled=='yes',1,0),[m
[32m+[m[32m         views = ifelse(views==0,NA,views)) %>%[m[41m [m
[32m+[m[32m  filter(., tracks > 0, views > listens)[m
[32m+[m
[32m+[m[32msummary(raw.data_1)[m
[32m+[m[32m```[m
[32m+[m[32m#### Drop Streaming Enabled because it's always 1[m
[32m+[m[32m```{r}[m
[32m+[m[32mraw.data_1 <- select(raw.data_1, !streaming_enabled)[m
[32m+[m[32m```[m
[32m+[m
[32m+[m[32m#### Investigating NA downloads[m
[32m+[m[32m```{r}[m
[32m+[m[32mraw.data_1 %>%[m
[32m+[m[32m  filter(., download_enabled==1, is.na(downloads))[m
[32m+[m[32m```[m
[32m+[m[32m#### Upon investigation of the site, these mixtapes have errors on their pages where downloads failed to load, along with views.[m
[32m+[m
[32m+[m[32m## Imputation Efforts[m
[32m+[m[32m#### Mixtapes with downloads enabled and NA downloads[m
[32m+[m[32m```{r}[m
[32m+[m[32mdl_able <- filter(raw.data_1, download_enabled==1)[m
[32m+[m[32mno_dl_able <- filter(raw.data_1, download_enabled==0)[m
[32m+[m
[32m+[m[32mdl_able_impute <- VIM::kNN(dl_able, variable = 'downloads', k = 5, imp_var = FALSE)[m
[32m+[m
[32m+[m[32mraw.data_2 <- full_join(no_dl_able, dl_able_impute)[m
[32m+[m
[32m+[m[32msummary(raw.data_2)[m
[32m+[m[32m```[m
[32m+[m
[32m+[m[32m#### Impute Mixtapes with 0 views[m
[32m+[m[32m```{r}[m
[32m+[m[32mdata_no_dl <- raw.data_2 %>%[m[41m [m
[32m+[m[32m  mutate(., views = ifelse(views==0,NA,views)) %>%[m[41m [m
[32m+[m[32m  select(., !(downloads))[m
[32m+[m
[32m+[m[32mdata_no_views <- raw.data_2 %>%[m[41m [m
[32m+[m[32m  select(., !(views))[m
[32m+[m
[32m+[m[32mimpute_views_no_dl <- VIM::kNN(data_no_dl, variable = 'views', imp_var = FALSE, k = 5)[m
[32m+[m
[32m+[m[32mraw.data_3 <- full_join(impute_views_no_dl, data_no_views)[m
[32m+[m
[32m+[m[32mdata <- raw.data_3[m
[32m+[m[32m```[m
[32m+[m
[32m+[m[32m## Adding Features:[m[41m [m
[32m+[m[32m#### listens_per_view, downloads_per_view, release_month (in YYYY-MM format), release_week[m
[32m+[m[32m```{r}[m
[32m+[m[32mdata <- data %>%[m[41m [m
[32m+[m[32m  mutate(.,[m[41m [m
[32m+[m[32m         listens_per_view = listens / views,[m
[32m+[m[32m         downloads_per_view = downloads / views,[m
[32m+[m[32m         release_year = year(release_date),[m
[32m+[m[32m         release_month = floor_date(release_date, unit = "month"),[m[41m [m
[32m+[m[32m         release_week = floor_date(release_date, unit = 'week'))[m
[32m+[m[32m```[m
[32m+[m
[32m+[m[32m## Some analysis:[m
[32m+[m
[32m+[m[32m### Correlations[m
[32m+[m[32m```{r}[m
[32m+[m[32mcor_dl <- data %>%[m[41m [m
[32m+[m[32m  filter(., download_enabled == 1) %>%[m[41m [m
[32m+[m[32m  select(., !c(added_by, artist, banner, host, release_date,release_month, release_year, release_week, title,[m
[32m+[m[32m               download_enabled, buy_enabled)) %>%[m
[32m+[m[32m  cor(.)[m
[32m+[m
[32m+[m[32mcor_no_dl <- data %>%[m[41m [m
[32m+[m[32m  filter(., download_enabled == 0) %>%[m[41m [m
[32m+[m[32m  select(., !c(added_by, artist, banner, host, release_date,release_month, release_year, release_week, title,[m[41m [m
[32m+[m[32m               download_enabled, downloads, downloads_per_view)) %>%[m
[32m+[m[32m  cor(.)[m
[32m+[m
[32m+[m[32mcormap_to_map <- function(cormap)[m
[32m+[m[32m{[m
[32m+[m[32m  cormap[upper.tri(cormap)] <- NA[m
[32m+[m[32m  cormap <- melt(cormap, na.rm = T)[m
[32m+[m[32m  return(cormap)[m
[32m+[m[32m}[m[41m [m
[32m+[m
[32m+[m[32mcormap_dl_plot <- cormap_to_map(cor_dl)[m
[32m+[m
[32m+[m[32mcormap_no_dl_plot <- cormap_to_map(cor_no_dl)[m
[32m+[m
[32m+[m[32mmake_heatmap <- function(cormap, title){[m
[32m+[m[32m  out <- ggplot(cormap, aes(x = Var1, y = Var2, fill = value)) +[m[41m [m
[32m+[m[32m    geom_tile(color = "white") +[m
[32m+[m[32m    scale_fill_gradient2(low = "gray", high = "orange",[m[41m [m
[32m+[m[32m                         midpoint = 0.5, limit = c(0,1), space = "Lab",[m[41m [m
[32m+[m[32m                         name="Correlation") +[m
[32m+[m[32m    theme_minimal()+[m[41m [m
[32m+[m[32m    theme(axis.text.x = element_text(angle = 45, vjust = 1,[m[41m [m
[32m+[m[32m                                     size = 12, hjust = 1)) +[m
[32m+[m[32m    coord_fixed() +[m[41m [m
[32m+[m[32m    ggtitle(title)[m
[32m+[m[41m  [m
[32m+[m[32m  return(out)[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mheatmap_dl <- make_heatmap(cormap_dl_plot, "Correlation Heatmap for Mixtapes\nwith Downloads Enabled")[m
[32m+[m
[32m+[m[32mheatmap_no_dl <- make_heatmap(cormap_no_dl_plot, "Correlation Heatmap for Mixtapes\nwith Downloads Disabled")[m
[32m+[m[32m```[m
[32m+[m
[32m+[m[32m```{r}[m
[32m+[m[32mheatmap_dl[m
[32m+[m[32m```[m
[32m+[m[32m```{r}[m
[32m+[m[32mheatmap_no_dl[m
[32m+[m[32m```[m
[32m+[m
[32m+[m[32m#### Grouping Data monthly + Plotting[m
[32m+[m[32m```{r}[m
[32m+[m[32mmonthly <- data %>%[m[41m [m
[32m+[m[32m  group_by(., release_month) %>%[m[41m [m
[32m+[m[32m  summarise(., count = n(),[m[41m [m
[32m+[m[32m            avg_views = mean(views), med_views = median(views),[m
[32m+[m[32m            avg_listens = mean(listens), med_listens = median(listens),[m[41m [m
[32m+[m[32m            avg_downloads = mean(downloads, na.rm = T), med_downloads = median(downloads, na.rm = T),[m
[32m+[m[32m            avg_lpv = mean(listens_per_view), med_lpv = median(listens_per_view),[m
[32m+[m[32m            avg_dpv = mean(downloads_per_view, na.rm = T), med_dpv = median(downloads_per_view, na.rm = T))[m
[32m+[m[32m```[m
[32m+[m
[32m+[m[32m```{r}[m
[32m+[m[32mmonthly_stats <- monthly %>%[m[41m [m
[32m+[m[32m  pivot_longer(.,[m
[32m+[m[32m               count:med_dpv,[m
[32m+[m[32m               names_to = "stat",[m
[32m+[m[32m               values_to = "value")[m
[32m+[m[32m```[m
[32m+[m
[32m+[m[32m#### Plotting Setup[m
[32m+[m[32m```{r}[m
[32m+[m[32mview_stats = c("avg_views", "med_views")[m
[32m+[m[32mdownload_stats = c("avg_downloads", "med_downloads")[m
[32m+[m[32mlisten_stats = c("avg_listens", "med_listens")[m
[32m+[m[32mfreq_stats = c("count")[m
[32m+[m[32mlpv_stats = c("avg_lpv", "med_lpv")[m
[32m+[m[32mdpv_stats = c("avg_dpv", "med_dpv")[m
[32m+[m[32mlistens_downloads = c("avg_views", "avg_listens", "avg_downloads")[m
[32m+[m[32mlpv_dpv = c("avg_lpv", "avg_dpv")[m
[32m+[m
[32m+[m[32mstat_plot <- function(x){[m
[32m+[m[32m  monthly_stats %>%[m[41m [m
[32m+[m[32m    filter(., stat %in% x) %>%[m
[32m+[m[32m    ggplot(., aes(x = release_month, y = value)) +[m
[32m+[m[32m    geom_line(aes(color = stat))[m[41m [m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m```[m
[32m+[m
[32m+[m[32m### Mixtape upload frequency over time[m
[32m+[m[32m```{r}[m
[32m+[m[32mstat_plot(freq_stats) +[m
[32m+[m[32m  ggtitle("Mixtape Uploads per Month") +[m
[32m+[m[32m  ylab("Mixtapes Uploaded") +[m[41m [m
[32m+[m[32m  xlab("Month") +[m
[32m+[m[32m  theme_minimal() +[m[41m [m
[32m+[m[32m  theme(legend.position = "none") +[m
[32m+[m[32m  scale_x_date(date_labels = "%b-%y", date_breaks = "year", limits = c(ymd("2010-01-01"),NA)) +[m[41m [m
[32m+[m[32m  scale_y_continuous(breaks = seq(0,150,25))[m
[32m+[m
[32m+[m[32m```[m
[32m+[m
[32m+[m[32m## Average and median mixtape views over time[m
[32m+[m[32m```{r}[m
[32m+[m[32mstat_plot(view_stats) +[m[41m [m
[32m+[m[32m  ggtitle('Mixtape Views by Release Month') +[m[41m [m
[32m+[m[32m  xlab("Month") +[m
[32m+[m[32m  ylab("Views") +[m[41m [m
[32m+[m[32m  theme_minimal() +[m[41m [m
[32m+[m[32m  scale_x_date(date_labels = "%b-%y", date_breaks = "2 years", limits = c(ymd("2009-06-30"),NA)) +[m
[32m+[m[32m  scale_y_continuous(limits = c(0, 1250000), breaks = seq(0,1250000, 250000), label = comma) +[m[41m [m
[32m+[m[32m  scale_color_discrete(name = NULL, labels = c("Monthly Average", "Monthly Median"))[m
[32m+[m
[32m+[m[32m```[m
[32m+[m
[32m+[m[32m## Average and median mixtape listens over time[m
[32m+[m[32m```{r}[m
[32m+[m[32mstat_plot(listen_stats) +[m[41m [m
[32m+[m[32m  ggtitle('Mixtape Listens by Release Month') +[m[41m [m
[32m+[m[32m  xlab("Release Month") +[m
[32m+[m[32m  ylab("Listens") +[m[41m [m
[32m+[m[32m  theme_minimal() +[m[41m [m
[32m+[m[32m  scale_x_date(date_labels = "%b-%y", date_breaks = "2 years", limits = c(ymd("2009-06-30"), NA)) +[m
[32m+[m[32m  scale_y_continuous(limits = c(0, 550000), labels = comma, breaks = seq(0,55E5,1E5)) +[m[41m [m
[32m+[m[32m  scale_color_discrete(name = NULL, labels = c("Monthly Average", "Monthly Median"))[m
[32m+[m[32m```[m
[32m+[m
[32m+[m[32m## Average and median mixtape downloads over time[m
[32m+[m[32m```{r}[m
[32m+[m[32mstat_plot(download_stats) +[m[41m [m
[32m+[m[32m  ggtitle("Mixtape Downloads by Release Month") +[m[41m [m
[32m+[m[32m  xlab("Release Month") +[m
[32m+[m[32m  ylab("Downloads") +[m[41m [m
[32m+[m[32m  theme_minimal() +[m[41m [m
[32m+[m[32m  scale_x_date(date_labels = "%b-%y", date_breaks = "2 years", limits = c(ymd("2009-06-30"), NA)) +[m
[32m+[m[32m  scale_y_continuous(limits = c(0, 200000), labels = comma) +[m[41m [m
[32m+[m[32m  scale_color_discrete(name = NULL, labels = c("Monthly Average", "Monthly Median"))[m
[32m+[m[32m```[m
[32m+[m
[32m+[m[32m## Average and median mixtape listens per view over time[m
[32m+[m[32m```{r}[m
[32m+[m[32mstat_plot(lpv_stats) +[m[41m [m
[32m+[m[32m  ggtitle("Listen per View Rate by Release Date") +[m[41m [m
[32m+[m[32m  xlab("Release Month") +[m
[32m+[m[32m  ylab("Listens per View") +[m[41m [m
[32m+[m[32m  theme_minimal() +[m[41m [m
