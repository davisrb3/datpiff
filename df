Binary files /dev/null and b/datpiff/spiders/__pycache__/datpiff_spider_0.cpython-38.pyc differ
[1mdiff --git a/datpiff/spiders/datpiff_spider.py b/datpiff/spiders/datpiff_spider.py[m
[1mnew file mode 100644[m
[1mindex 0000000..1e107fb[m
[1m--- /dev/null[m
[1m+++ b/datpiff/spiders/datpiff_spider.py[m
[36m@@ -0,0 +1,163 @@[m
[32m+[m[32mfrom scrapy import Spider[m
[32m+[m[32mfrom datpiff.items import DatpiffItem[m
[32m+[m[32mfrom scrapy import Request[m
[32m+[m[32mimport re[m
[32m+[m
[32m+[m[32mclass datpiff_spider(Spider):[m
[32m+[m[32m    name = 'datpiff_spider'[m
[32m+[m[32m    allowed_urls = ['https://www.datpiff.com/'][m
[32m+[m[32m    start_urls = ['https://www.datpiff.com/mixtapes.php?filter=all&p=1'][m
[32m+[m
[32m+[m[32m    def parse(self, response):[m
[32m+[m[32m    # Get current page number and next page number for iteration[m
[32m+[m[32m    # Page numbers continue endlessley, even if pages are blank[m
[32m+[m[32m    # 'Next page' button on the last populated page points to itself, this is how end of mixtapes is detected[m
[32m+[m[32m        current_page = int(response.xpath('//div[@class="pagination"]//a[@class="active"]/text()').extract_first())[m
[32m+[m[32m        next_page = int(str.split(response.xpath('//div[@class="pagination"]/a[@class="next"]/@href').extract_first(),'=')[-1])[m
[32m+[m[32m        next_page_url = 'https://www.datpiff.com' + response.xpath('//div[@class="pagination"]/a[@class="next"]/@href').extract_first()[m
[32m+[m
[32m+[m[32m        #Get Mixtape boxes on page[m
[32m+[m[32m        mixtapes = response.xpath('//div[@id="leftColumnWide"]//div[@class="contentItemInner"]')[m
[32m+[m[41m        [m
[32m+[m[32m        for mixtape in mixtapes:[m
[32m+[m[41m            [m
[32m+[m[32m            banners = mixtape.xpath('./a/div/@class').extract()[m
[32m+[m
[32m+[m[32m            # Only scrape official mixtapes[m
[32m+[m[32m            if "banner official" not in banners:[m
[32m+[m[32m                continue[m
[32m+[m[32m            #     official = "official"[m
[32m+[m[32m            # else:[m[41m [m
[32m+[m[32m            #     official = "unofficial"[m
[32m+[m
[32m+[m[32m            if "banner sponsor" in banners:[m
[32m+[m[32m                banner = "sponsored"[m
[32m+[m[32m            elif "banner exclusive" in banners:[m
[32m+[m[32m                banner = "exclusive"[m
[32m+[m[32m            else:[m
[32m+[m[32m                banner = "none"[m
[32m+[m
[32m+[m[32m            mixtape_url = 'https://www.datpiff.com' + mixtape.xpath('.//div[@class="title"]//@href').extract_first()[m
[32m+[m[32m            artist = mixtape.xpath('./div[@class="artist"]/text()').extract_first().strip()[m
[32m+[m[32m            title = mixtape.xpath('./div[@class="title"]//text()').extract_first().strip()[m
[32m+[m
[32m+[m[32m            try:[m
[32m+[m[32m                listens = int(''.join(re.findall('\d+', mixtape.xpath('./div[text()="Listens: "]/span/text()').extract_first())))[m
[32m+[m[32m            except Exception as e:[m
[32m+[m[32m                print(type(e), e)[m
[32m+[m[32m                listens = "failed"[m
[32m+[m
[32m+[m[32m            try:[m
[32m+[m[32m                rating_score = int(re.split(' ',mixtape.xpath('./div[@class="text"]/img/@alt').extract_first())[0])[m
[32m+[m[32m            except Exception as e:[m
[32m+[m[32m                print(type(e), e)[m
[32m+[m[32m                rating_score = "failed"[m
[32m+[m
[32m+[m[32m            try:[m
[32m+[m[32m                rating_count = int(''.join(re.findall('\d+', re.split(' ',mixtape.xpath('./div[@class="text"]/img/@title').extract_first())[0])))[m
[32m+[m[32m            except Exception as e:[m
[32m+[m[32m                print(type(e), e)[m
[32m+[m[32m                rating_count = "failed"[m
[32m+[m
[32m+[m[32m            meta = {'artist' : artist,[m
[32m+[m[32m                    'title' : title,[m
[32m+[m[32m                    'listens' : listens,[m
[32m+[m[32m                    'rating_score': rating_score,[m
[32m+[m[32m                    'rating_count' : rating_count,[m
[32m+[m[32m                    'banner' : banner,[m
[32m+[m[32m                    # 'official' : official[m
[32m+[m[32m                    }[m
[32m+[m[32m            yield Request(url = mixtape_url, callback = self.parse_mixtape_page, meta = meta)[m
[32m+[m[32m        if current_page < next_page:[m
[32m+[m[32m            yield Request(url = next_page_url, callback = self.parse)[m
[32m+[m
[32m+[m[32m    def parse_mixtape_page(self, response):[m
[32m+[m[32m        info = response.xpath('//div[@class="module1"]')[m
[32m+[m[32m        listens = response.meta['listens'][m
[32m+[m
[32m+[m[32m        try:[m
[32m+[m[32m            host = str.strip(info.xpath('.//li[@class="dj"]/text()').extract_first())[m[41m   [m
[32m+[m[32m        except Exception as e:[m
[32m+[m[32m            print(type(e),e)[m
[32m+[m[32m            host = None[m
[32m+[m
[32m+[m[32m        try:[m
[32m+[m[32m            views = ''.join(re.findall('\d+', info.xpath('.//li[@class="listens"]/text()').extract_first()))[m
[32m+[m[32m        except Exception as e:[m
[32m+[m[32m            print(type(e),e)[m
[32m+[m[32m            views = None[m
[32m+[m
[32m+[m[32m        try:[m
[32m+[m[32m            release_date = info.xpath('//div[@class = "left"]//span/text()').extract_first()[m
[32m+[m[32m        except Exception as e:[m
[32m+[m[32m            print(type(e),e)[m
[32m+[m[32m            release_date = None[m
[32m+[m
[32m+[m[32m        try:[m
[32m+[m[32m            added_by = info.xpath('//div[@class = "left"]//a/text()').extract_first()[m
[32m+[m[32m        except Exception as e:[m
[32m+[m[32m            print(type(e),e)[m
[32m+[m[32m            added_by = None[m
[32m+[m
[32m+[m[32m        try:[m
[32m+[m[32m            description = str.strip(info.xpath('.//div[@class="description"]//text()').extract_first())[m
[32m+[m[32m        except Exception as e:[m
[32m+[m[32m            print(type(e),e)[m
[32m+[m[32m            description = ''[m
[32m+[m[41m        [m
[32m+[m[32m        try:[m
[32m+[m[32m            stat = [re.split('\-|\.',value)[-2] for value in info.xpath('.//div[@class="downloads right"]//li/img/@src').extract()][m
[32m+[m[32m            count = [str.strip(x) for x in info.xpath('.//div[@class="downloads right"]//li/text()').extract()][m
[32m+[m[32m            stats = {stat[x]:count[x] for x in range(0,len(stat))}[m
[32m+[m[32m            # listens = stats['listens'][m
[32m+[m[32m            if listens == "failed":[m
[32m+[m[32m                listens = int(''.join(re.findall('\d+', stats['listens'])))[m
[32m+[m[32m            downloads = int(''.join(re.findall('\d+', stats['downloads'])))[m
[32m+[m[32m        except Exception as e:[m
[32m+[m[32m            print(type(e),e)[m
[32m+[m[32m            downloads = None[m
[32m+[m
[32m+[m[32m        try:[m
[32m+[m[32m            tracks = len(response.xpath('//span[@class="tracknumber"]/text()').extract())[m
[32m+[m[32m        except Exception as e:[m
[32m+[m[32m            print(type(e),e)[m
[32m+[m[32m            tracks = len(response.xpath('//span[@class="tracknumber"]/text()').extract())[m
[32m+[m[41m        [m
[32m+[m[32m        buttons = response.xpath('//div[@class="actionButtons"]//text()').extract()[m
[32m+[m
[32m+[m[32m        if "Stream" in buttons:[m
[32m+[m[32m            streaming_enabled = "yes"[m
[32m+[m[32m        else:[m
[32m+[m[32m            streaming_enabled = "no"[m
[32m+[m
[32m+[m[32m        if "Download" in buttons:[m
[32m+[m[32m            download_enabled = "yes"[m
[32m+[m[32m        else:[m
[32m+[m[32m            download_enabled = "no"[m
[32m+[m
[32m+[m[32m        if "BUY" in buttons:[m
[32m+[m[32m            buy_enabled = "yes"[m
[32m+[m[32m        else:[m
[32m+[m[32m            buy_enabled = "no"[m
[32m+[m
[32m+[m
[32m+[m[32m        item = DatpiffItem()[m
[32m+[m[32m        item['title'] = response.meta['title'][m
[32m+[m[32m        item['artist'] = response.meta['artist'][m
[32m+[m[32m        # item['official'] = response.meta['official'][m
[32m+[m[32m        item['downloads'] = downloads[m
[32m+[m[32m        # item['award'] = award[m
[32m+[m[32m        item['host'] = host[m
[32m+[m[32m        item['views'] = views[m
[32m+[m[32m        item['release_date'] = release_date[m
[32m+[m[32m        item['added_by'] = added_by[m
[32m+[m[32m        item['description'] = description[m
[32m+[m[32m        item['listens'] = listens[m
[32m+[m[32m        item['tracks'] = tracks[m
[32m+[m[32m        item['rating_count'] = response.meta['rating_count'][m
[32m+[m[32m        item['rating_score'] = response.meta['rating_score'][m
[32m+[m[32m        item['banner'] = response.meta['banner'][m
[32m+[m[32m        item['streaming_enabled'] = streaming_enabled[m
[32m+[m[32m        item['download_enabled'] = download_enabled[m
[32m+[m[32m        item['buy_enabled'] = buy_enabled[m
[32m+[m[32m        yield item[m[41m  [m
\ No newline at end of file[m
